fn fizz_buzz(n) {
    while (n > 0) {
        if (n % 3 == 0 && n % 5 == 0) {
            print("FizzBuzz\n");
        } else if (n % 3 == 0) {
            print("Fizz\n");
        } else if (n % 5 == 0) {
            print("Buzz\n");
        } else {
            print("%d\n", n);
        }
        n = n - 1;
    }
}

fn main() {
   fizz_buzz(10);
}