fn main() {
                let a = malloc(4 * 10); // Allocate memory for 10 integers
                let i = 0;
                while (i < 10) {
                    *(a + i) = i * 2; // Store even numbers
                    i += 1;
                }
                i = 0;
                while (i < 10) {
                    print("%d ", *(a + i)); // Print the values
                    i += 1;
                }
                free(a); // Free the allocated memory
            }